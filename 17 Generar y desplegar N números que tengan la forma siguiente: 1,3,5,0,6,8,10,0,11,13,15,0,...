#include <iostream>

using namespace std;

int main() {
    int N;

    cout << "Ingrese el valor de N: ";
    cin >> N;

    cout << "Los primeros " << N << " nÃºmeros de la serie son: ";
    int num = 1;
    int count = 0;
    for (int i = 1; i <= N; i++) {
        if (count == 3) {
            cout << 0 << " ";
            count = 0;
            num += 5;  // Skip to the next odd number after 0
        } else {
            cout << num << " ";
            num += 2;
            count++;
        }
    }
    cout << endl;

    return 0;
}
