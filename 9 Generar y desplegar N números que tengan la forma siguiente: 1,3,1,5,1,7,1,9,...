#include <iostream>

using namespace std;

int main() {
    int N;

    cout << "Ingrese el valor de N: ";
    cin >> N;

    cout << "Los primeros " << N << " nÃºmeros de la serie son: ";
    for (int i = 1; i <= N; i++) {
        if (i % 2 == 1) {
            cout << 1 << " ";
        } else {
            cout << i + 1 << " ";
        }
    }
    cout << endl;

    return 0;
}
