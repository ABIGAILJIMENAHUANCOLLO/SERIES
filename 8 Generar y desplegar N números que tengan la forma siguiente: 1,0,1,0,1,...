#include <iostream>

using namespace std;

int main() {
    int N;

    cout << "Ingrese el valor de N: ";
    cin >> N;

    cout << "Los primeros " << N << " nÃºmeros de la serie son: ";
    for (int i = 1; i <= N; i++) {
        cout << (i % 2) << " "; // (i % 2) will be 1 for odd i and 0 for even i
    }
    cout << endl;

    return 0;
}
