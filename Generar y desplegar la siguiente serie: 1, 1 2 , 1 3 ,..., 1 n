#include <iostream>

using namespace std;

int main() {
    int n;

    cout << "Ingrese el valor de n: ";
    cin >> n;

    for (int i = 1; i <= n; i++) {
        cout << "1" << endl;
        for (int j = 2; j <= i; j++) {
            cout << j << endl;
        }
        if (i < n) {
            cout << "," << endl;
        }
    }

    return 0;
}
