#include <iostream>

using namespace std;

int main() {
    int N;

    cout << "Ingrese el valor de N: ";
    cin >> N;

    cout << "Los primeros " << N << " nÃºmeros de la serie son: ";
    int num = 1;
    int count = 0; 
    for (int i = 1; i <= N; i++) {
        if (count == 1) {
            count = 0; 
            num++;     // Move to the next number
        }
        cout << num << " ";
        num += (count == 0) ? 2 : 1; // Add 2 if count is 0, otherwise add 1
        count++;
    }
    cout << endl;

    return 0;
}
